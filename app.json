{
  "expo": {
    "name": "Estoque",
    "slug": "Estoque",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/estoque.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme":"Estoque",
    "splash": {
      "image": "./assets/loading-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/estoque.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/estoque.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
